<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<StackPane prefHeight="205.0" prefWidth="383.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx"
           fx:controller="layout.adapter.AuthFXMLAdapter">
    <children>
        <StackPane>
            <children>
                <VBox spacing="10.0">
                    <children>
                        <GridPane>
                            <children>
                                <Label text="Tên đăng nhập" GridPane.columnIndex="0" GridPane.rowIndex="0"/>
                                <Label text="Mật khẩu" GridPane.columnIndex="0" GridPane.rowIndex="1"/>
                                <TextField fx:id="userName" GridPane.columnIndex="1" GridPane.rowIndex="0"/>
                                <PasswordField fx:id="password" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
                            </children>
                            <columnConstraints>
                                <ColumnConstraints hgrow="SOMETIMES" maxWidth="171.99996337890624" minWidth="10.0"
                                                   prefWidth="105.60000762939453"/>
                                <ColumnConstraints hgrow="SOMETIMES" maxWidth="257.5999923706055" minWidth="10.0"
                                                   prefWidth="257.5999923706055"/>
                            </columnConstraints>
                            <rowConstraints>
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                            </rowConstraints>
                        </GridPane>
                        <StackPane prefHeight="-1.0" prefWidth="-1.0">
                            <children>
                                <Button fx:id="loginButton" alignment="CENTER" defaultButton="true"
                                        mnemonicParsing="false" prefHeight="26.0" prefWidth="87.0" text="Đăng nhập"
                                        StackPane.alignment="CENTER_RIGHT"/>
                            </children>
                        </StackPane>
                    </children>
                </VBox>
            </children>
        </StackPane>
    </children>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>
</StackPane>
